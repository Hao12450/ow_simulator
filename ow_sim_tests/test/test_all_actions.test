<?xml version="1.0"?>
<launch>

    <arg name="gzclient" default="true" />

    <include file="$(find ow)/launch/europa_terminator_workspace.launch">
        <arg name="rqt_gui" value="true" />
        <arg name="use_rviz" value="false"/>
        <arg name="gzclient" value="$(arg gzclient)"/>
    </include>

    <test test-name="test_task_scoop_linear" pkg="ow_sim_tests"
        type="test_task_scoop_linear.py" time-limit="700.0"/>

    <test test-name="test_task_scoop_circular" pkg="ow_sim_tests"
        type="test_task_scoop_circular.py" time-limit="700.0"/>

    <test test-name="test_task_grind" pkg="ow_sim_tests"
        type="test_task_grind.py" time-limit="700.0"/>

    <test test-name="test_task_discard_sample" pkg="ow_sim_tests"
        type="test_task_discard_sample.py" time-limit="700.0"/>

    <test test-name="test_task_deliver_sample" pkg="ow_sim_tests"
        type="test_task_deliver_sample.py" time-limit="700.0"/>

    <test test-name="pan_tilt_move_joints" pkg="ow_sim_tests"
        type="test_pan_tilt_move_joints.py" time-limit="700.0"/>

    <test test-name="pan_tilt_move_cartesian" pkg="ow_sim_tests"
        type="test_pan_tilt_move_cartesian.py" time-limit="700.0"/>

    <test test-name="test_light_set_intensity" pkg="ow_sim_tests"
        type="test_light_set_intensity.py" time-limit="700.0"/>

    <test test-name="test_camera_set_exposure" pkg="ow_sim_tests"
        type="test_camera_set_exposure.py" time-limit="700.0"/>

    <test test-name="test_camera_capture" pkg="ow_sim_tests"
        type="test_camera_capture.py" time-limit="700.0"/>

    <test test-name="arm_stop" pkg="ow_sim_tests"
        type="test_arm_stop.py" time-limit="700.0"/>

    <test test-name="test_arm_move_joint" pkg="ow_sim_tests"
        type="test_arm_move_joint.py" time-limit="700.0"/>

    <test test-name="test_arm_move_joints" pkg="ow_sim_tests"
        type="test_arm_move_joints.py" time-limit="700.0"/>

    <test test-name="test_arm_move_joints_guarded" pkg="ow_sim_tests"
        type="test_arm_move_joints_guarded.py" time-limit="700.0"/>

    <test test-name="arm_move_cartesian_guarded" pkg="ow_sim_tests"
        type="test_arm_move_cartesian_guarded.py" time-limit="700.0"/>

    <test test-name="test_arm_find_surface" pkg="ow_sim_tests"
        type="test_arm_find_surface.py" time-limit="700.0"/>

    <test test-name="arm_unstow" pkg="ow_sim_tests"
        type="test_arm_unstow.py" time-limit="700.0"/>

    <test test-name="arm_move_cartesian" pkg="ow_sim_tests"
        type="test_arm_move_cartesian.py" time-limit="700.0"/>

</launch>
